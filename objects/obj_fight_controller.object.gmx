            <string>/// Move cursor

if (global.is_fight) {
    if (keyboard_check_pressed(ord('W')) || keyboard_check_pressed(vk_up)) {
        if (boxs &gt; 1) {
            boxs -= 1;
        }
    }
    if (keyboard_check_pressed(ord('S')) || keyboard_check_pressed(vk_down)) {
        if (boxs &lt; 4) {
            boxs += 1;
        }
    }
    
    switch(boxs) {
        case 1:
            box1 = true;
            box2 = false;
            box3 = false;
            box4 = false;
            break;
        case 2:
            box1 = false;
            box2 = true;
            box3 = false;
            box4 = false;
            break;
        case 3:
            box1 = false;
            box2 = false;
            box3 = true;
            box4 = false;
            break;
        case 4:
            box1 = false;
            box2 = false;
            box3 = false;
            box4 = true;
            break;
    }
}
</string>
